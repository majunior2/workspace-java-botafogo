package zoo;

public class App {

	public static void main(String[] args) {
		Gato gato = new Gato();
		Cachorro cao= new Cachorro ();
		Jacare jaca = new Jacare ();
		
		jaca.setPeso(100);
		jaca.setRaca("Papo amarelo");
		jaca.brincar();
		jaca.emitirSom();
		System.out.println("O jacar√© pesa : " +jaca.getPeso());
		
		cao.brincar();
		
		gato.setCor ("Laranja");
		gato.setRaca ("Vira lata");
		gato.setPeso (2);
		
	}

}
