package zoo;

public class App {

	public static void main(String[] args) {
		Gato gato = new Gato();
		Cachorro cachorro = new Cachorro ();
		Jacare jaca = new Jacare ();
		
		jaca.setRaca("Papo amarelo");
		jaca.setpeso(100);
		jaca.brincar();
		
		gato.setCor ("Laranja");
		gato.setRaca ("Vira lata");
		gato.setPeso (2);
		gato.saltar();
		gato.emitirSom();

	}

}
