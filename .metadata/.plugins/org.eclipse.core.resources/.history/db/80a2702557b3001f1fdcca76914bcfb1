package zoo;

public class App {

	public static void main(String[] args) {
		Gato gato = new Gato();
		Cachorro cao= new Cachorro ();
		Jacare jaca = new Jacare ();
		
		jaca.setRaca("Papo amarelo");
		jaca.setPeso(100);
		jaca.brincar();
		jaca.emitirSom();
		jaca.saltar();
		
		cao.emitirSom();
		cao.brincar();
		cao.setPeso(35);
		cao.setRaca("Pastor");
		
		gato.setCor ("Laranja");
		gato.setRaca ("Vira lata");
		gato.setPeso (2);
		gato.saltar();
		gato.emitirSom();

	}

}
